   "source": [
    "# Entropy-Gated Irreversible Growth Simulation\n",
    "Langevin-style irreversible growth with entropy export gating"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Config\n",
    "T = 200\n",
    "dt = 0.1\n",
    "alpha = 1.0\n",
    "S_max = 10\n",
    "gamma_values = [0.0, 0.02, 0.05, 0.1]\n",
    "stochastic = False\n",
    "noise_std = 0.5\n",
    "\n",
    "results = {}\n",
    "for gamma in gamma_values:\n",
    "    g = np.zeros(T)\n",
    "    S = np.zeros(T)\n",
    "    dg_dt = np.zeros(T)\n",
    "    for t in range(1, T):\n",
    "        eta = np.random.normal(loc=1.0, scale=noise_std) if stochastic else 1.0\n",
    "        dg = eta if S[t-1] < S_max else 0\n",
    "        dg_dt[t] = dg\n",
    "        g[t] = g[t-1] + dg * dt\n",
    "        dS = alpha * (dg**2) * dt\n",
    "        S[t] = S[t-1] + dS - gamma * S[t-1] * dt\n",
    "    results[gamma] = {"g": g, "S": S, "dg_dt": dg_dt}\n",
    "\n",
    "# Plot\n",
    "fig, axes = plt.subplots(1, 3, figsize=(18,5))\n",
    "for gamma in gamma_values:\n",
    "    axes[0].plot(results[gamma]["g"], label=f"γ={gamma}")\n",
    "axes[0].set_title("Irreversible Growth g(t)")\n",
    "axes[0].legend()\n",
    "for gamma in gamma_values:\n",
    "    axes[1].plot(results[gamma]["S"], label=f"γ={gamma}")\n",
    "axes[1].set_title("Entropy S(t)")\n",
    "axes[1].legend()\n",
    "for gamma in gamma_values:\n",
    "    axes[2].plot(results[gamma]["dg_dt"], label=f"γ={gamma}")\n",
    "axes[2].set_title("Growth Rate dg/dt")\n",
    "axes[2].legend()\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  }
